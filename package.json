{
  "name": "@eroscripts/funlib",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "bun@1.2.22",
  "description": "A library for working with .funscript files",
  "author": "Dimava",
  "license": "MIT",
  "keywords": [
    "funscript"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    }
  },
  "module": "src/index.ts",
  "scripts": {
    "lint": "eslint --fix",
    "build:iife": "bun build src/iife.ts --target browser --outdir dist --format iife",
    "build:esm": "bun build src/index.ts --target browser --outfile dist/funlib.js --format esm",
    "build:dts": "bunx dts-bundle-generator -o dist/funlib.d.ts src/index.ts",
    "build": "bun run build:esm && bun run build:dts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "colorizr": "^3.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/bun": "latest",
    "@types/w3c-web-serial": "^1.0.8",
    "bun-types": "^1.2.11"
  }
}
